msgid "<p><span class=\"RktErr\">f:expects 1 argument, found none</span></p>"
msgstr ""

#: in_1/part_one.html:1641
msgid ""
"</td></tr><tr><td><span class=\"stt\">&gt; </span><span class=\"RktPn\">(</"
"span><span class=\"RktSym\">f</span><span class=\"hspace\">&nbsp;</"
"span><span class=\"RktVal\">1</span><span class=\"hspace\">&nbsp;</"
"span><span class=\"RktVal\">2</span><span class=\"hspace\">&nbsp;</"
"span><span class=\"RktVal\">3</span><span class=\"hspace\">&nbsp;</"
