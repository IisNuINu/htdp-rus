"span><span class=\"hspace\">&nbsp;</span><span class=\"RktPn\">(</span><span "
"class=\"RktSym\">find-next-state</span><span class=\"hspace\">&nbsp;</"
"span><span class=\"RktSym\">an-fsm</span><span class=\"hspace\">&nbsp;</"
"span><span class=\"RktSym\">ke</span><span class=\"RktPn\">)</span></td></"
"tr><tr><td><span class=\"hspace\">&nbsp;&nbsp;</span><span class=\"RktPn"
"\">(</span><span class=\"RktSym\">make-fs</span></td></tr><tr><td><span "
"class=\"hspace\">&nbsp;&nbsp;&nbsp;&nbsp;</span><span class=\"RktPn\">(</"
"span><span class=\"RktSym\">fs-fsm</span><span class=\"hspace\">&nbsp;</"
"span><span class=\"RktSym\">an-fsm</span><span class=\"RktPn\">)</span></"
"td></tr><tr><td><span class=\"hspace\">&nbsp;&nbsp;&nbsp;&nbsp;</span><span "
"class=\"RktPn\">(</span><span class=\"RktSym\">find</span><span class="
"\"hspace\">&nbsp;</span><span class=\"RktPn\">(</span><span class=\"RktSym"
"\">fs-fsm</span><span class=\"hspace\">&nbsp;</span><span class=\"RktSym"
"\">an-fsm</span><span class=\"RktPn\">)</span><span class=\"hspace\">&nbsp;</"
